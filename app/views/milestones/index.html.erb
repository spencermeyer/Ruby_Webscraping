<p id="notice"><%= notice %></p>

<h1>Milestones</h1>

<table>
  <thead>
    <tr>
      <th>Pos</th>
      <th>Parkrunner</th>
      <th>Time</th>
      <th>Age cat</th>
      <th>Age grade</th>
      <th>Gender</th>
      <th>Gender pos</th>
      <th>Club</th>
      <th>Note</th>
      <th>Total</th>
      <th>Run</th>
      <th>Age grade position</th>
      <th>Age cat position</th>
      <th>Athlete number</th>
      <th>Integer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @milestones.each do |milestone| %>
      <tr>
        <td><%= milestone.pos %></td>
        <td><%= milestone.parkrunner %></td>
        <td><%= milestone.time %></td>
        <td><%= milestone.age_cat %></td>
        <td><%= milestone.age_grade %></td>
        <td><%= milestone.gender %></td>
        <td><%= milestone.gender_pos %></td>
        <td><%= milestone.club %></td>
        <td><%= milestone.note %></td>
        <td><%= milestone.total %></td>
        <td><%= milestone.run_id %></td>
        <td><%= milestone.age_grade_position %></td>
        <td><%= milestone.age_cat_position %></td>
        <td><%= milestone.athlete_number %></td>
        <td><%= milestone.integer %></td>
        <td><%= link_to 'Destroy', milestone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


